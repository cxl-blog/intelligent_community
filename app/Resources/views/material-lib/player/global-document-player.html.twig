{% extends 'layout.html.twig' %}

{% block body %}
  {% if setting("developer.cloud_sdk_cdn") %}
    {% set cdnUrl = setting("developer.cloud_sdk_cdn")  %}
  {% else %}
    {% set cdnUrl = '//service-cdn.qiqiuyun.net'  %}
  {% endif %}
  {% do script([cdnUrl~'/js-sdk/sdk-v1.js','app/js/material-lib/global-document-player/index.js']) %}
  <div class="iframe-parent-content" id="global-player"
       data-res-no="{{ globalId }}"
       data-token="{{ token }}"
       data-user-id="{{app.user.id|default(0)}}"
       data-user-name="{{app.user.username|default('')}}"
       data-client-type="{{ app.request.get('clientType')|default('app') }}"
  ></div>
{% endblock %}

